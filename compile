#-quicklisp
(let ((quicklisp-init (merge-pathnames "quicklisp/setup.lisp"
                                       (user-homedir-pathname))))
  (when (probe-file quicklisp-init)
    (load quicklisp-init)))
(ql:quickload :marad)

(in-package :marad)
#+SBCL
(sb-ext:save-lisp-and-die "marad" :executable t :toplevel '_start)
#-SBCL
(error "not implemented")
